
<article class="module width_full">
	<% @brand.errors.full_messages.each do |e|%>
		<%=e%>

		<% end unless @brand.errors.blank?%>
	<div class="breadcrumbs_container">
			<article class="breadcrumbs"><%= link_to "Brand Listing", admin_brands_path(current_user.admin)%>
				<div class="breadcrumb_divider"></div>
				 <a class="current">Edit</a>
			</article>
		</div>
	<header></header>
	<%= form_for [current_user.admin,@brand] do |f|%>		
		<div class="module_content post_message">	
				<div>			
					<label>Name</label>
					<%=f.text_field :name%>					
					<label>Code</label>
					<%=f.text_field :code%>
				</div>
				<div class="clear"></div>
				<div>			
					<label>Address</label>
					<%=f.text_field :address%>
					<label>Contact</label>
					<%=f.text_field :contact%>
				</div>
				<div>			
					<label>Dealin</label>
					<%=f.text_field :dealin%>
					<label>Description</label>
					<%=f.text_field :description%>				
				</div>
				<div>			
					<label>Founder</label>
					<%=f.text_field :founder%>
					<label>Is Active</label>
					<%=f.select :isactive, [[true,true],[false, false]]%>				
				</div>
		</div>
		<footer>
			<div class="submit_link">
				<%=f.submit "Update"%>
			</div>
		</footer>
    <% end %>
	
</article>